            <groupId>org.jodd</groupId>
            <artifactId>jodd-core</artifactId>
            <version>3.5</version>
        </dependency>
        <dependency>
            <groupId>org.jodd</groupId>
            <artifactId>jodd-http</artifactId>
            <version>3.5</version>
        </dependency>
        <!-- ] jodd end -->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>fastjson</artifactId>
            <version>1.1.38</version>
